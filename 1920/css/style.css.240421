  50%{background : conic-gradient(var(--color-pie2) 0% 72%, #ffffff00 0% 100%)}
  56%{background : conic-gradient(var(--color-pie2) 0% 74%, #ffffff00 0% 100%)}
  62%{background : conic-gradient(var(--color-pie2) 0% 76%, #ffffff00 0% 100%)}
  68%{background : conic-gradient(var(--color-pie2) 0% 78%, #ffffff00 0% 100%)}
  75%{background : conic-gradient(var(--color-pie2) 0% 80%, #ffffff00 0% 100%)}
  86%{background : conic-gradient(var(--color-pie2) 0% 83%, #ffffff00 0% 100%)}
  92%{background : conic-gradient(var(--color-pie2) 0% 86%, #ffffff00 0% 100%)}
  100%{background : conic-gradient(var(--color-pie2) 0% 90%, #ffffff00 90% 100%)}
}

@keyframes pie3 {
  0%{background : conic-gradient(var(--color-pie3) 90% 90%, #ffffff00 0% 100%)}
  10%{background : conic-gradient(var(--color-pie3) 90% 91%, #ffffff00 0% 100%)}
