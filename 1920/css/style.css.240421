  25%{background : conic-gradient(var(--color-pie1) 0% 17%, #ffffff00 0% 100%)}
  33%{background : conic-gradient(var(--color-pie1) 0% 21%, #ffffff00 0% 100%)}
  38%{background : conic-gradient(var(--color-pie1) 0% 24%, #ffffff00 0% 100%)}
  44%{background : conic-gradient(var(--color-pie1) 0% 27%, #ffffff00 0% 100%)}
  50%{background : conic-gradient(var(--color-pie1) 0% 30%, #ffffff00 0% 100%)}
  56%{background : conic-gradient(var(--color-pie1) 0% 34%, #ffffff00 0% 100%)}
  62%{background : conic-gradient(var(--color-pie1) 0% 37%, #ffffff00 0% 100%)}
  68%{background : conic-gradient(var(--color-pie1) 0% 41%, #ffffff00 0% 100%)}
  75%{background : conic-gradient(var(--color-pie1) 0% 44%, #ffffff00 0% 100%)}
  82%{background : conic-gradient(var(--color-pie1) 0% 47%, #ffffff00 0% 100%)}
  88%{background : conic-gradient(var(--color-pie1) 0% 50%, #ffffff00 0% 100%)}
  94%{background : conic-gradient(var(--color-pie1) 0% 53%, #ffffff00 0% 100%)}
  100%{background : conic-gradient(var(--color-pie1) 0% 56%, #ffffff00 56% 100%)}
}

@keyframes pie2 {
  0%{background : conic-gradient(var(--color-pie2) 0% 56%, #ffffff00 0% 100%)}
  6%{background : conic-gradient(var(--color-pie2) 0% 58%, #ffffff00 0% 100%)}
  12%{background : conic-gradient(var(--color-pie2) 0% 60%, #ffffff00 0% 100%)}
  18%{background : conic-gradient(var(--color-pie2) 0% 62%, #ffffff00 0% 100%)}
  25%{background : conic-gradient(var(--color-pie2) 0% 64%, #ffffff00 0% 100%)}
  33%{background : conic-gradient(var(--color-pie2) 0% 66%, #ffffff00 0% 100%)}
  38%{background : conic-gradient(var(--color-pie2) 0% 68%, #ffffff00 0% 100%)}
