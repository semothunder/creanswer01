/* style.css */

:root {
  --color-white: #ffffff;
  --color-black: #1f1f29;
  --color-light-gray: #bcbcbd;
  --color-gray: #89898a;
  --color-gray-02: #414245;
  --color-purple-01: #44149d;
  --color-purple-02: #8158ba;
  --color-purple-02-5: #73559c;
  --color-purple-03: #9c90b4;
  --color-yellow: #fac55d;
  --color-new-blue: #124fe8;
  --color-blue : #1b76ff;

  --color-m-yellow :#fede29;

  --color-pie1 : #313653 ;
  --color-pie2 : #545c8a;
  --color-pie3 : #9097c0;

/* #313653 */

/* #545c8a */

/* #9097c0 */

  --swiper-theme-color: #bcbcbd;

  --max-width: 1920px;
  --max-width-02: 1980px;
  /* --base-width: 1230px; */
  --base-width: 1300px;
  --base-width-02: 1392px;
  --base-height: 1080px;
}

.sec-base {
  width: var(--base-width);
  box-sizing: border-box;
  margin: auto;
}

.sec-base-02 {
  width: var(--base-width-02);
  box-sizing: border-box;
  margin: auto;
}

.sec-wrapper {
  width: var(--base-width);
  margin: auto;
  position: relative;
}

#container {
  position: relative;
  width: 100%;
}

.title-black {
  font-size: 44px;
  font-weight: 400;
  /* letter-spacing: -0.025em; */
  line-height: 1.5;
  text-align: center;
  /* fs : 58px lh : 78px */
}
.title-black.c3-title {
  font-weight: 800;
}

.title-black > p.bold {
  font-weight: 800;
}

.text-01 {
  font-size:20px;
  line-height: 25px;
  font-weight: 500;
  margin-bottom: 20px;
  letter-spacing: -0.025em;
}
.text-effect {
  -webkit-text-stroke: 2px var(--color-black);
  text-shadow: -1px 0px var(--color-black), 0px 1px var(--color-black), 1px 0px var(--color-black), 0px -1px var(--color-black);
  color: #fff;
}

.text-blue {
  color: var(--color-blue);
}

/* font */

/* main.css */


/* section-main */

#main {
  padding-bottom: 60px;
  height: 751px;
  background-color: #1b181d;
}

.sec_base {
  width: 100vw;
  height: 846px;
  position: relative;
  overflow: hidden;
  display: flex;
  justify-content: center;
  padding: 24px 0;
}


/* text */
.title_text {
  font-size: 44.8px;
  letter-spacing: -4px;
  font-weight: 800;
}

#main .small_text {
  font-size: 18px;
  padding-top: 101px;
  text-indent: 13px;
  animation: mainTextMove 1200ms ease 2200ms;
  animation-fill-mode: forwards;
  opacity: 0;
  font-weight: 500;
  color: var(--color-white);
}

#main .small_text.bottom {
  padding-top: 96px;
  font-size: 27px;
  text-indent: 7px;
  /* letter-spacing: -0.02em; */
  animation: mainTextMove 1200ms ease 2700ms;
  animation-fill-mode: forwards;
  color: #fff;
  line-height: 1.55;
}

#main .small_text.bottom > img {
  width: 25.6px;
}
#main {
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  margin: auto;
  max-width: 100%;
}

#main>.container_01 {
  padding-top: 190px;
  height: 100%;
  position: relative;
  margin: auto;
  width: var(--base-width);
  z-index: 100;
}

@-webkit-keyframes mainTextMove {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 1;
    padding-bottom: 0px;
  }
}

@keyframes mainTextMove {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 1;
    padding-bottom: 0px;
  }
}

#main .section-01-bg {
  width: 55%;
  object-position: center;
  z-index: -1;
  opacity: 0.2;
}

.typing-txt {
  display: none;
  margin: 0 auto;
  height: 0px;
  color: var(--color-white);
}

.t-title {
  list-style: none;
  height: 150px;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
}

.t-title>li {
  display: block;
  font-weight: 900;
  line-height: 1.3;
  letter-spacing: -0.48px;
  color: var(--color-white);
}

.typing {
  /* width:100%; */
  font-size:79px;
}

.typing ul li:nth-child(-n+2) {
  font-weight: 900;
  letter-spacing: -0.02em;
  word-break: keep-all;
  line-height: 0.9;
}

.typing ul li:nth-child(1) {
  color: var(--color-white);
}

.t-title>li:nth-child(-n+2) {
  text-shadow: -1px -1px 0 var(--color-blue), 1px -1px 0 var(--color-blue), -1px 1px 0 var(--color-blue), 1px 1px 0 var(--color-blue);
}

.typing ul li.on {
  display: inline-block;
  animation-name: cursor;
  animation-duration: 0.3s;
  animation-iteration-count: infinite;
}

@keyframes cursor {
  0% {
    border-right: 1px solid #fff
  }

  50% {
    border-right: 1px solid #000
  }

  100% {
    border-right: 1px solid #fff
  }
}

.main-img-wrapper {
  position: absolute;
  width: 825px;
  bottom: 177px;
  right: -180px;

}

.main-phone-img {
  width: 577px;
  object-position: center;
  z-index: -1;
}

.main-img-text {
  position: absolute;
  font-weight: 600;
  font-size: 16px;
  line-height: 1.25;
  letter-spacing: -0.8px;
  color: var(--color-white);
}

.main-img-text.number-01 {
  top: 24px;
  left: 311px;
}
.main-img-text.number-02 {
  top: 432px;
  left: -152px;
  text-align: right;
}
.main-img-text.number-03 {
  top: 473px;
  left: 494px;
}

.t-title>li:nth-child(3) {
    margin-top: 27.2px;
    height: 80px;
    color: var(--color-blue);
    font-size: 64px;
    letter-spacing: -1px;
}

#main .sample-li.text {
  padding-bottom: 42px;
}

.main-img-roll-wrapper {
  top: -6%;
  right: 0;
  width: 700px;
  z-index: 50;
  height: 1807px;
  overflow: hidden;
}

.main-img-roll-slide {
  top: 6%;
  width:100%;
  animation: mainSlideMove 27000ms linear both infinite;
}
.main-img-roll {
  width:100%;
  opacity: 0.07;
}
@keyframes mainSlideMove {
  0%{ top: 0%;}
  100%{ top: -1807px;}
}

#main .text-blue {
  font-weight: 800;
}


/* ====================== */

@-webkit-keyframes mainIconMove {
  0% {
    -webkit-transform: translateY(0);
            transform: translateY(0);
  }

  50% {
    -webkit-transform: translateY(10px);
            transform: translateY(10px);
  }

  100% {
    -webkit-transform: translateY(0);
            transform: translateY(0);
  }
}

@keyframes mainIconMove {
  0% {
    -webkit-transform: translateY(0);
            transform: translateY(0);
  }

  50% {
    -webkit-transform: translateY(10px);
            transform: translateY(10px);
  }

  100% {
    -webkit-transform: translateY(0);
            transform: translateY(0);
  }
}



